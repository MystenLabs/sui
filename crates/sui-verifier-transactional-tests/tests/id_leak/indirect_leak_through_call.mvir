//# publish
// not allowed since Foo is not packed with a fresh UID
module 0x0.m {
    import 0x2.object;

    struct Foo has key {
        id: object.UID,
    }

    transfer(id: Self.Foo) {
        label l0:
        abort 0;
    }

    foo(f: Self.Foo, v: &mut vector<object.UID>) {
        let id: object.UID;
        label l0:
        Foo { id } = move(f);
        Self.transfer(Foo { id: move(id) });
        return;
    }

}

