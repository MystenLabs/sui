module 0x1.Test {
    struct X { b: bool }
    enum T { V{ i: u64, x: Self.X } }

    public new_t(): Self.T {
        let x: Self.X;
    label b0:
        x = X { b: true };
        return T.V { i: 0, x: move(x) };
    }

    public destroy_t(t: Self.T): u64 * Self.X * bool {
        let i: u64;
        let x: Self.X;
        let flag: bool;
    label b0:
        // extra binding, invalid positive stack size
        T.V { i, x, b: flag } = move(t);
        return move(i), move(x), move(flag);
    }

}

