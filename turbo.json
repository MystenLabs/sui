{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"lint": {},
		"test": {
			"dependsOn": ["^build", "build"],
			"outputs": ["coverage/**"]
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**", "dist/**", "storybook-static/**", ".next/**", "!.next/cache/**", "pkg/**"]
		}
	},
	"globalEnv": ["VERCEL_ENV", "VITE_NETWORK", "VITE_VERCEL_ANALYTICS_ID", "VITE_SENTRY_SAMPLE_RATE"]
}
